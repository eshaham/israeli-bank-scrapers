{
  "name": "israeli-bank-scrapers",
  "version": "0.9.0",
  "description": "Provide scrapers for all major Israeli banks and credit card companies",
  "engines": {
    "node": ">= 8.2.1",
    "npm": ">= 5.3.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=test jest",
    "test:ci": "ncp src/tests/.tests-config.tpl.ts src/tests/.tests-config.ts && npm run test",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "dev": "npm run type-check -- --watch",
    "build": "npm run lint && npm run clean && npm run build:types && npm run build:js && npm run build:puppeteer-config",
    "build:types": "tsc --emitDeclarationOnly",
    "build:puppeteer-config": "cp src/puppeteer-config.json lib",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline --verbose",
    "prebuild": "node utils/update-puppeteer-config.js",
    "postbuild": "rimraf lib/tests",
    "prepare:core": "node utils/prepare-israeli-bank-scrapers-core.js && npm i --package-lock-only && npm ci && npm run build",
    "prepare:default": "npm ci && npm run build",
    "reset": "git reset --hard && npm ci",
    "publish:core": "npm run prepare:core && npm publish --access public && npm run reset",
    "publish:default": "npm run prepare:default && npm test && npm publish --access public && npm run reset",
    "publish:all": "npm run publish:default && npm run publish:core"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eshaham/israeli-bank-scrapers.git"
  },
  "keywords": [
    "israel",
    "israeli bank",
    "israeli bank scraper"
  ],
  "author": "Elad Shaham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eshaham/israeli-bank-scrapers/issues"
  },
  "homepage": "https://github.com/eshaham/israeli-bank-scrapers#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node-fetch": "^2.5.6",
    "@types/puppeteer": "^2.0.1",
    "@types/source-map-support": "^0.5.1",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-import-visitor": "^0.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.3.0",
    "json2csv": "^4.2.1",
    "ncp": "^2.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "build-url": "^2.0.0",
    "core-js": "^3.1.4",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-fetch": "^2.2.0",
    "uuid": "^3.3.2",
    "puppeteer": "^2.0.0"
  },
  "files": [
    "lib/**/*"
  ]
}
